# RF  interlocks
record(calc, "$(P):sHB-CNT") {
   field (INPA, "$(P):sHB")
   field (INPB, "$(P):sHB-CNT.LA")
   field (CALC, "(A#B)?VAL+1:VAL")
   field (SCAN, "1 second")
}

record(calc, "$(P):sAlive") {
   field (INPA, "$(P):sHB-CNT")
   field (INPB, "$(P):sAlive.LA")
   field (INPC, "$(P):cArm")
   field (CALC, "((A#B)&&C)?1:0")
   field (SCAN, "5 second")
}


record(bo, "$(P):Reset") {
   field (HIGH, "0.5")
   field (ZNAM, "Iddle")
   field (ONAM, "CmdReset")
   field (PINI, "YES")
   field (OUT, "$(P):sHB-CNT")
}

record(aSub, "$(P)-trg:sTime-aSub")
{
  field(DESC, "LabVIEW time stamp decoding")
  field(INPA, "$(P)-trg:sTime MSI")
  field(FTA, "DOUBLE")
  field(FTVA, "STRING")
  field(OUTA, "$(P)-trg:sTimeStamp MS")
  field(FLNK, "$(P)-trg:sTimeStamp PP")
  field(SNAM, "myAsubProcess")
  field(SCAN, "1 second")
}

record(stringin, "$(P)-trg:sTimeStamp")
{
  field(DESC, "Time stamp string")
}
