# Sonar device

record(ai, "Cryo-Rec:LP:GasBagSonar-new") {
   field (DESC, "Distance sensor: to gas bag")
   field (EGU, "cm")
   field (DTYP, "stream")
   field (LOPR, "0")
   field (HOPR, "300.0")
   field (PREC, "0")
   field (INP, "@Sonar.proto get_distance $(PORT2)")
   field (SCAN, "5 second")
}
